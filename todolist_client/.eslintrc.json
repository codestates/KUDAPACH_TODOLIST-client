{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended, react/react-in-jsx-scope",
    "some-other-config-you-use",
    "eslint"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "curly": ["error", "all"],
    "no-unused-vars": 2,
    "no-undef": 2,
    "no-console": 2
  }
}
